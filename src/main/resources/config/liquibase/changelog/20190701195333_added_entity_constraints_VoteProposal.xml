<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.6.xsd">
    <!--
        Added the constraints for entity VoteProposal.
    -->
    <changeSet id="20190701195333-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="proposal_id"
                                 baseTableName="vote_proposal"
                                 constraintName="fk_vote_proposal_proposal_id"
                                 referencedColumnNames="id"
                                 referencedTableName="proposal"/>

        <addForeignKeyConstraint baseColumnNames="profile_id"
                                 baseTableName="vote_proposal"
                                 constraintName="fk_vote_proposal_profile_id"
                                 referencedColumnNames="id"
                                 referencedTableName="profile"/>

    </changeSet>
</databaseChangeLog>
